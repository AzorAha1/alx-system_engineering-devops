global
    ssl-dh-param-file /etc/haproxy/dhparams.pem


frontend serverfront
    mode http
    bind *:443 ssl crt /etc/haproxy/certs/www.Techwithfaiz.tech.pem
    redirect scheme https if !{ ssl_fc }
    default_backend serverback
backend serverback
    mode http
    balance roundrobin
    server 415758-web-01 54.197.131.36 check
    server 415758-web-02 100.26.226.35 check
